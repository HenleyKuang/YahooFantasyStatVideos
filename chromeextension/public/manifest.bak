{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://basketball.fantasysports.yahoo.com/*"
            ],
            "css": ["./static/css/modal.css"],
            "js": ["./static/js/content.js"],
            "run_at": "document_end",
            "all_frames": true
        },
        {
            "matches": [
                "*://basketball.fantasysports.yahoo.com/*"
            ],
            "css": ["./static/css/videojs.css"],
            "js": ["./static/js/video.js"],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "content_security_policy": {
        "extension_pages": "default-src 'self'; connect-src https://* data: blob: filesystem:;"
    },
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "https://yahoo-fantasy-bball-stat-video.herokuapp.com/*",
        "*://*/*"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo512.png",
            "32": "logo512.png",
            "48": "logo512.png",
            "128": "logo512.png"
        }
    },
    "icons": {
        "16": "favicon.ico",
        "32": "favicon.ico",
        "48": "favicon.ico",
        "128": "favicon.ico"
    },
    "web_accessible_resources": [{
        "resources": ["/static/js/video.js", "/static/js/video.js.map"],
        "matches": ["<all_urls>"]
    }]
  }