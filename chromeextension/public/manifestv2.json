{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["static/js/content_scripts.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "<all_urls>"
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo512.png",
            "32": "logo512.png",
            "48": "logo512.png",
            "128": "logo512.png"
        }
    },
    "icons": {
        "16": "favicon.ico",
        "32": "favicon.ico",
        "48": "favicon.ico",
        "128": "favicon.ico"
    },
    "web_accessible_resources": [
        "*.js",
        "*.map"
    ]
  }