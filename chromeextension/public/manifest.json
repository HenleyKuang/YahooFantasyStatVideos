{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": [
            "*://basketball.fantasysports.yahoo.com/*"
          ],
          "css": ["/static/css/main.css"],
          "js": ["/static/js/main.js"],
          "run_at": "document_end",
          "all_frames": true
        }
    ],
    "content_security_policy": {
        "extension_pages": "default-src 'self'; connect-src https://* data: blob: filesystem:;"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": ["https://yahoo-fantasy-bball-stat-video.herokuapp.com/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo512.png",
            "32": "logo512.png",
            "48": "logo512.png",
            "128": "logo512.png"
        }
    },
    "icons": {
        "16": "favoicon.ico",
        "32": "favoicon.ico",
        "48": "favoicon.ico",
        "128": "favoicon.ico"
    }
  }